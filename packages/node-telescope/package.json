{
  "name": "node-telescope",
  "version": "1.0.0",
  "description": "A logging and monitoring library for node.js",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc && npm run build:frontend",
    "build:frontend": "cd ../node-telescope-frontend && npm run build && cross-env-shell \"bash ../node-telescope/scripts/copy-frontend.sh\"",
    "watch": "npm run clean && tsc --watch",
    "dev": "npm run watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "pack:telescope": "npm pack",
    "clean:prod-test": "rimraf ../examples/prod-test/node_modules ../examples/prod-test/package-lock.json",
    "setup:prod-test": "npm run pack:telescope && cross-env-shell \"bash scripts/check-tgz-exists.sh\"",
    "install:prod-test": "cross-env-shell \"bash scripts/install-prod-test.sh\"",
    "start:prod-test": "cd ../examples/prod-test && npm start",
    "prod-test": "npm run build && npm run clean:prod-test && npm run setup:prod-test && npm run install:prod-test && npm run start:prod-test",
    "prepublishOnly": "npm run build",
    "postbuild": "cd ../node-telescope-frontend && cross-env-shell \"bash scripts/postbuild.sh\""
  },
  "author": "mdipanjan",
  "license": "MIT",
  "keywords": [
    "nodejs",
    "monitoring",
    "debugging",
    "telescope",
    "logging",
    "performance",
    "express",
    "middleware",
    "database",
    "query-logging"
  ],
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "socket.io": "^4.7.5",
    "uuid": "^10.0.0",
    "winston": "^3.13.0"
  },
  "peerDependencies": {
    "express": "^4.19.2",
    "mongoose": "^8.4.4",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^26.0.24",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^14.14.41",
    "@types/pg": "^8.11.6",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^3.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-jest": "^27.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
    "mongodb": "^6.8.0",
    "pg": "^8.12.0",
    "prettier": "^2.2.1",
    "rimraf": "^5.0.9",
    "socket.io-client": "^4.7.5",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mdipanjan/node-telescope.git"
  },
  "bugs": {
    "url": "https://github.com/mdipanjan/node-telescope/issues"
  },
  "homepage": "https://github.com/mdipanjan/node-telescope#readme"
}
